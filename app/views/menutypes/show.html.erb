<h2>Type de menu</h2>
<% for column in Menutype.content_columns %>
<p>
  <b><%= column.human_name %>:</b> <%=h @menutype.send(column.name) %>
</p>
<% end %>

<div class="post-footer">	
<p class="links">
<%= link_to 'Editer', edit_menutype_path(@menutype) %> |
<%= link_to 'Retour', menutypes_url %>
</p>
</div>