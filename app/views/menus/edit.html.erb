<h1>Editer</h1>
<% form_for @menu, :url => menus_path do |f| %>
  <%= messages_error @menu %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "Update" %>
<% end %>

<div class="post-footer">	
<p class="links">
<%= link_to "Voir", :action => "show", :id => @menu %> |
<%= link_to "Retour", :action => "list" %>
</p>
</div>
