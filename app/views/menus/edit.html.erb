<h1>Editer</h1>
<% form_for @menu, :url => menus_path do |f| %>
  <%= messages_error @menu %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "Update" %>
<% end %>

<div class="post-footer">	
<p class="links">
<%= link_to "Voir", menu_path(@menu) %> |
<%= link_to "Retour", menus_url %>
</p>
</div>
