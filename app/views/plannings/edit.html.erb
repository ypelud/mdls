<h1>Editer</h1>
<% form_for @planning, :url => plannings_path do |f| %>
  <%= messages_error(@planning) %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit I18n.t("planning.update"), :class => 'button' %>
<% end %>


<div class="post-footer">	
<p class="links">
<%= link_to "Retour", :action => "index" %>
</p>
</div>
